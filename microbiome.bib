	doi = {10.1371/journal.pone.0010957},
	journal = {PLOS ONE},
	journal-iso = {PLoS One},
	keywords-plus = {YELLOW-LEGGED FROG; BATRACHOCHYTRIUM-DENDROBATIDIS; HYDROGEN-PEROXIDE; INFECTIOUS-DISEASES; POPULATION DECLINES; PEPTIDE DEFENSES; CHYTRIDIOMYCOSIS; ECOLOGY; TEMPERATURE; DIVERSITY},
	month = {JUN 4},
