	date-modified = {2025-03-01 21:06:16 -1000},
	doi = {10.1016/j.jprot.2019.103623},
	journal = {JOURNAL OF PROTEOMICS},
	journal-iso = {J. Proteomics},
	keywords = {Microbiome functions; Functional diversity; Bacteria; Fungi; Protein-coding sequences},
	keywords-plus = {CARBON SEQUESTRATION; ECOLOGY; GENOME},
