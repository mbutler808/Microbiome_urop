	keywords = {microbiome; community assembly; Batrachochytrium dendrobatidis; defensive symbiosis; chytridiomycosis; Rana sierrae},
	keywords-plus = {EMERGING INFECTIOUS-DISEASE; PANAMANIAN GOLDEN FROGS; YELLOW-LEGGED FROG; BACTERIAL COMMUNITIES; PELAGIC BACTERIA; FUNGAL PATHOGEN; GUT MICROBIOTA; POPULATION; CHYTRIDIOMYCOSIS; DIVERSITY},
	month = {MAR 21},
	times-cited = {63},
	title = {Host and Aquatic Environment Shape the Amphibian Skin Microbiome but Effects on Downstream Resistance to the Pathogen <i>Batrachochytrium dendrobatidis</i> Are Variable},
