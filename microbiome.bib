	journal-iso = {Appl. Environ. Microbiol.},
	keywords = {Amazon; fish; gut; habitat; host; microbiome; phylosymbiosis; skin; species; teleost},
	keywords-plus = {CORAL-REEF FISH; GASTROINTESTINAL MICROBIOTA; BACTERIAL COMMUNITIES; DIVERGENT SELECTION; LIFE-HISTORY; RESILIENCE; RESISTANCE; SPECIATION; DIVERSITY; ECOLOGY},
	month = {AUG},
